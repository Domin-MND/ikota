{
  "name": "ikota",
  "version": "0.6.6",
  "description": "CLI automation tool for working with React",
  "author": "Domin-MND",
  "bin": {
    "ikota": "./bin/run"
  },
  "homepage": "https://github.com/Domin-MND/ikota",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Domin-MND/ikota.git"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/color": "^1.0.4",
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-not-found": "^2.3.20",
    "inquirer": "^8.2.5"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.6",
    "@types/chai": "^4",
    "@types/inquirer": "^8.2.6",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.12",
    "@types/supports-color": "^8.1.1",
    "chai": "^4",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "ikota",
    "dirname": "ikota",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ],
    "hooks": {
      "init": "./dist/hooks/initConfig"
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b && shx cp ./src/templates/styling/styles.module.* ./dist/templates/styling",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "prettier:check": "prettier --check \"**/*.ts\"",
    "prettier:write": "prettier --write \"**/*.ts\"",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/Domin-MND/ikota/issues",
  "keywords": [
    "automation",
    "react",
    "front-end",
    "web",
    "cli"
  ],
  "types": "dist/index.d.ts"
}
